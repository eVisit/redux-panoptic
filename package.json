{
  "name": "redux-panoptic",
  "version": "0.0.7",
  "description": "Redux via templates, by eVisit",
  "main": "dist/store.js",
  "scripts": {
    "example": "node node_modules/babel-cli/bin/babel-node examples/test.js",
    "clean": "node node_modules/rimraf/bin.js dist",
    "build": "node node_modules/babel-cli/bin/babel.js --presets=env --plugins=transform-object-rest-spread src --out-dir dist",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eVisit/redux-panoptic.git"
  },
  "keywords": [
    "evisit",
    "redux"
  ],
  "author": "eVisit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eVisit/redux-panoptic/issues"
  },
  "homepage": "https://github.com/eVisit/redux-panoptic#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-env": "^1.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "redux": "^3.7.2",
    "evisit-js-utils": "^0.4.13"
  }
}
